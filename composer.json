{
    "name": "nhockizi/yii2-cms",
    "version": "dev-master",
    "description": "CMS based on Yii2 Framework - Development package",
    "keywords": ["nhockizi"],
    "homepage": "https://github.com/nhockizi/yii2-cms",
    "type": "yii2-extension",
    "license": "GNU GPL V3",
    "minimum-stability": "dev",
    "support": {
        "source": "https://github.com/nhockizi/yii2-cms.git",
        "issues": "https://github.com/nhockizi/yii2-cms/issues"
    },
    "authors": [
        {
            "name": "Lâm Xuân Bảo",
            "email": "nhockizi@gmail.com",
            "country": "Viet Nam"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "autoload": {
        "psr-4": {
            "nhockizi\\cms\\": "/"
        }
    }, 
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
